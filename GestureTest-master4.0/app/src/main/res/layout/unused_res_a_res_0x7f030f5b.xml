    android:layout_height="wrap_content"
    android:background="@drawable/unused_res_a_res_0x7f02151b"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/unused_res_a_res_0x7f0a1360"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="20dp"
        android:src="@drawable/unused_res_a_res_0x7f021528" />

    <ProgressBar
        android:id="@+id/unused_res_a_res_0x7f0a1361"
