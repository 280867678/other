<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="160dp"
    android:layout_height="30dp"
    android:background="@drawable/unused_res_a_res_0x7f0210c0">

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/unused_res_a_res_0x7f0a059a"
        android:layout_width="18dp"
        android:layout_height="18dp"
        android:layout_gravity="center_vertical"
        android:layout_marginLeft="12dp"
        app:lottie_autoPlay="false"
        app:lottie_fileName="player_brightness_lottie.json"
        app:lottie_loop="false" />

    <ProgressBar
        android:id="@+id/unused_res_a_res_0x7f0a059b"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_gravity="center_vertical"
        android:layout_marginLeft="36dp"
        android:layout_marginRight="12dp"
        android:indeterminateOnly="false"
        android:progress="20"
        android:progressDrawable="@drawable/unused_res_a_res_0x7f021039" />
</FrameLayout>
