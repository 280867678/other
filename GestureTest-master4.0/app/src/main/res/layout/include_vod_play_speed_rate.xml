<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/vod_speed_rate_container"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingLeft="@dimen/vod_play_bottom_bar_text_padding"
    android:paddingRight="@dimen/vod_play_bottom_bar_text_padding"
    android:visibility="gone">

    <yanzhikai.gesturetest.popup.VodSpeedRateTextView
        android:id="@+id/vod_speed_rate_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:includeFontPadding="false"
        android:padding="4dp"
        android:textColor="@color/ui_force_white"
        android:textSize="12sp" />
</FrameLayout>
